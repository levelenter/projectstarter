  "main": "index.js",
  "scripts": {
    "c:dev": "cd client/ && bun run dev",
    "c:start": "cd client/ && bun run start",
    "c:build": "cd client/ && bun run build", 
    "c:preview": "cd client/ && bun run preview",
    "c:astro": "cd client/ && bun run astro",
    "s:dev": "cd server/ && bun run dev",
    "s:build": "cd server/ && bun run build",
    "s:kill": "cd server/ && bun run kill",
    "prod": "npm run c:build && npm run s:build ",
    "all": "cd framework && bun run lib && bun run entity && bun run rest && bun run sql",
    "lib": "ts-node --project ./tsconfig.fw.json ./framework/generator/copy_lib/index.ts",
    "entity": "ts-node --project ./tsconfig.fw.json ./framework/generator/entity/index.ts",
    "rest": "ts-node --project ./tsconfig.fw.json ./framework/generator/rest/index.ts",
    "sql": "ts-node --project ./tsconfig.fw.json  ./framework/generator/sql/index.ts"
